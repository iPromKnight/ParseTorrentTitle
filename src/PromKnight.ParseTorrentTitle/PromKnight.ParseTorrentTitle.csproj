<Project Sdk="Microsoft.NET.Sdk.Worker">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <Version>1.0.4</Version>
        <IsPackable>true</IsPackable>
    </PropertyGroup>

    <PropertyGroup>
        <Title>PromKnight.ParseTorrentTitle</Title>
        <Authors>iPromKnight</Authors>
        <Description>A port of PTN from python to C#.</Description>
        <PackageProjectUrl>https://github.com/iPromKnight/ParseTorrentTitle</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/iPromKnight/ParseTorrentTitle.git</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageTags>ptn;parsetorrenttitle</PackageTags>
        <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.1" />
    </ItemGroup>

</Project>
